@startuml
'https://plantuml.com/class-diagram

class ListManager {
ToDoList : Collection

AddList()
DeleteList()
SaveList()
LoadList()
}

class ToDoList {
name : String
Item : Collection

AddItem()
DeleteItem()
SaveList()
LoadList()
SortList()
}

class Item {
name : String
desc : String
completed : boolean
duedate : int

EditName(name)
EditDesc(desc)
EditCompletion(boolean)
EditDueDate(int)
}

ToDoList -- Item
ListManager -- ToDoList

@enduml